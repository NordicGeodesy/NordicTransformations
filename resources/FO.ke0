<FVR09> proj=vgridshift grids=fvr09g2012.gtx

<UTM29N> proj=utm zone=29 ellps=GRS80 units=m no_defs
<UTM29N_FVR09> proj=pipeline step init=FO:FVR09 step init=FO:UTM29N

<FOTM> proj=etmerc lat_0=0 lon_0=-7 k=0.999997 x_0=200000 y_0=-6000000 ellps=GRS80 units=m no_defs
<FOTM_FVR09> proj=pipeline step init=FO:FVR09 step init=FO:FOTM
<FKE> proj=lmb lat_0=62 lon_0=-9 x_0=500000 y_0=500000 lat_1=62 ellps=GRS80 units=m no_defs
<FKE_FVR09> proj=pipeline step init=FO:FVR09 step init=FO:FKE

# ??
<FD54_formal> proj=utm zone=29 ellps=intl units=m no_defs
<FD50_formal> proj=utm zone=29 ellps=intl units=m no_defs
<FD89_formal> proj=utm zone=29 ellps=intl units=m no_defs

<FG54_formal> proj=geo ellps=intl units=dg no_defs
<FG50_formal> proj=geo ellps=intl units=dg no_defs

<FU54_formal> proj=utm zone=29 ellps=intl units=m no_defs
<FU50_formal> proj=utm zone=29 ellps=intl units=m no_defs
<FU54_FVR09_formal> proj=pipeline step init=FO:FVR09 step init=FO:FU54
<FU50_FVR09_formal> proj=pipeline step init=FO:FVR09 step init=FO:FU50

<FK54_formal> proj=lmb lat_0=62 lon_0=-9 x_0=500000 y_0=500000 lat_1=62 ellps=intl units=m no_defs
<FK89_formal> proj=lmb lat_0=62 lon_0=-9 x_0=700000 y_0=700000 lat_1=62 ellps=intl units=m no_defs
<FK54_FVR09_formal> proj=pipeline step init=FO:FVR09 step init=FO:FK54
<FK54_FVR09_formal> proj=pipeline step init=FO:FVR09 step init=FO:FK54
