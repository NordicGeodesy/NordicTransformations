#------------------------------------------------------------------------------------------------------
# ETRS89 (Geodetic) -> Generalstabens System, except Bornholm
#------------------------------------------------------------------------------------------------------
<GS>  proj=pipeline  ellps=GRS80
    step   init=DK:UTM32_ED50_L
    step   init=DK_general.pol:GS
#------------------------------------------------------------------------------------------------------
<GEO_GS> proj=pipeline  ellps=GRS80
    step   init=DK:GS
    step   init=DK:GS_FORMAL inv
#------------------------------------------------------------------------------------------------------
