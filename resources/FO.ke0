# GEO_ETRS89 -> UTM29_ED50_FO
# Test point: -7 62 100 0 -> 604850.9786090221  6876031.0401384393   100.0     0
# echo -7 62 100 0 | cct init=FO.ke0:UTM29_ED50
#
<UTM29_ED50> proj=pipeline
    step   proj=utm ellps=GRS80 zone=29
    step   init=fu50.pol:UTM29_ED50

# GEO_ETRS89 -> FK54_FD54
# Test point: -7 62 100 0 -> 394920.18507748388  501535.28689490078   100.0  0.0
# echo -7 62 100 0 | cct +init=FO.ke0:FK54_FD54|cct -I +init=FO.ke0:FK54_FD54
<FK54_FD54>  proj=pipeline
    step proj=utm ellps=GRS80 zone=29
    step init=fk54.pol:FK54_FD54

# GEO_ETRS89  ->  FK89_FD89 (Really FD54a: FD54 revised in 1989, THIS IS NOT ETRS89!)
# Test point: -7 62 100 0 -> 594919.9029294271  701535.25742737006   100.0  0.0
# echo -7 62 100 0 | cct +init=FO.ke0:FK89_FD89|cct -I +init=FO.ke0:FK89_FD89
<FK89_FD89>  proj=pipeline
    step proj=utm ellps=GRS80 zone=29
    step init=fk89.pol:FK89_FD89
